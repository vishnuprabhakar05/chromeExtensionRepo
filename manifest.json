{
    "name": "Chrome Button Clicker",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup":"linkedinConnect.html",
        "default_icon": "logo.png"
    },
    "icons":{
        "128":"logo.png"
    },
    "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ]
}